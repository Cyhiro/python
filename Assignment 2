# Base Class: Vehicle
class Vehicle:
    def move(self):
        raise NotImplementedError("This method should be overridden.")

# Subclass: Car
class Car(Vehicle):
    def move(self):
        return "Driving ğŸš—"

# Subclass: Plane
class Plane(Vehicle):
    def move(self):
        return "Flying âœˆï¸"

# Subclass: Bicycle
class Bicycle(Vehicle):
    def move(self):
        return "Pedaling ğŸš´"

# Using polymorphism
vehicles = [Car(), Plane(), Bicycle()]

for vehicle in vehicles:
    print(vehicle.move())
