# Base Class: Vehicle
class Vehicle:
    def move(self):
        raise NotImplementedError("This method should be overridden.")

# Subclass: Car
class Car(Vehicle):
    def move(self):
        return "Driving 🚗"

# Subclass: Plane
class Plane(Vehicle):
    def move(self):
        return "Flying ✈️"

# Subclass: Bicycle
class Bicycle(Vehicle):
    def move(self):
        return "Pedaling 🚴"

# Using polymorphism
vehicles = [Car(), Plane(), Bicycle()]

for vehicle in vehicles:
    print(vehicle.move())
